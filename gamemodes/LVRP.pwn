public CreateDynamicActor(modelid, Float:X, Float:Y, Float:Z, Float:Rotation, vworld, bool:vulnera, type, varia, labelHead[])
{
	new tmpId = CreateActor(modelid, X, Y, Z, Rotation);
	SetActorVirtualWorld(tmpId,vworld);
	SetActorInvulnerable(tmpId,vulnera);
	actor[tmpId][pos][0]=X;
	actor[tmpId][pos][1]=Y;
	actor[tmpId][pos][2]=Z;
	actor[tmpId][world]=vworld;
	actor[tmpId][typeZ]=type;
	actor[tmpId][variable]=varia;
	actor[tmpId][label] = CreateDynamic3DTextLabel(labelHead,0xFFFFFFDD,actor[tmpId][pos][0], actor[tmpId][pos][1], actor[tmpId][pos][2]+1.05,NAME_DISTANCE,INVALID_PLAYER_ID,INVALID_VEHICLE_ID,0, actor[tmpId][world], -1, -1, 100.0);
	return tmpId;
}

public DestroyDynamicActor(actorid)
{
	if(IsValidActor(actorid))
