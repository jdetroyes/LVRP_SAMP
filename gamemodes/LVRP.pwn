{
    MoveDynamicObject(gate_Object[gateId],gate[gateId][pos][0],gate[gateId][pos][1],gate[gateId][pos][2],2.0,gate[gateId][pos][3],gate[gateId][pos][4],gate[gateId][pos][5]);
	return 1;
}

