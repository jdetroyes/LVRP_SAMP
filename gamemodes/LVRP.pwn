stock MinNumber(...)
{
	new count = numargs(), minnum=99999, argMin;
	for(new i; i < count; i ++)
	{
	    new temp = getarg(i);
	    if(temp !=0 && temp < minnum) {minnum = temp; argMin = i;}
	}
