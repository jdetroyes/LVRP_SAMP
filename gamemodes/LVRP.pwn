	mysql_query(MYSQL,sql);
	new tmp_sqlid = cache_get_field_content_int(0,"id");
	
    if(by==-1)
        {giveplayer="Serveur";}
	else
	    {GetPlayerName(by, giveplayer, sizeof(giveplayer));}

