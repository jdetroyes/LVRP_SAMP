		if(player_Dialog[playerid] == 7)
		{
		    new id = player_Variable[playerid];
		    msg_Client(playerid,COLOR_WHITE,"{FF2727}» Admin «{FFABAD} Positions du panneau publicitaire sauvegardées.");
