		else if(GetVehicleSpeed(VehiculeID) <= SPEED_HIGHT)
			{format(speed,sizeof(speed),"~r~%d",GetVehicleSpeed(VehiculeID));}
		else
			{format(speed,sizeof(speed),"~p~%d",GetVehicleSpeed(VehiculeID));}
        	
