	    {
	        if(stepid==1)
	        {
		        TogglePlayerControllable(playerid,true);
		        msg_Client(playerid,COLOR_JOB,"{78769D}» Job «{FFFFFF} Allez décharger la cargaison.");
		        if(job_City[playerid] == 0) SetPlayerCheckpoint(playerid,2493.6907,-2090.1719,13.5469,8.0);
