	        new namebot[64], stringlength = strlen(npcname);
			strmid(namebot, npcname, 12, stringlength);
			new idplayer = strval(namebot);
			if(job_TempVar[idplayer] == 1)
			{
				for(new car = 1; car <= totalVehicles; car++)
