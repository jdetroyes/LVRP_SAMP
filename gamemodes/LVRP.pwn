		DestroyActor(actorid);
		DestroyDynamic3DTextLabel(actor[actorid][label]);
	    actor[actorid][pos][0]=0;
		actor[actorid][pos][1]=0;
		actor[actorid][pos][2]=0;
		actor[actorid][typeZ]=-1;
		actor[actorid][variable]=0;
		actor[actorid][world]=0;
	}
	return 1;
}

