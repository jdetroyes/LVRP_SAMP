			{SafeSetVehicleHealth(carid,vehicle[carid][cHealth]);}
		if(panels != vehicle[carid][cPanels] || doors != vehicle[carid][cDoors] || lights != vehicle[carid][cLights] || tires != vehicle[carid][cTires])
